#include <stdio.h>

long long int sum_of_fibonacci(int n) {
    if (n == 0)
        return 0;
    else if (n == 1)
        return 1;
    else
        return sum_of_fibonacci(n - 1) + sum_of_fibonacci(n - 2);
}

int main() {
    int n;
    printf("Vendosni vleren e n: ");
    scanf("%d", &n);
    printf("Shuma e %d numrit te pare te Fibonaccit eshte : %lld\n", n, sum_of_fibonacci(n));
    return 0;
}
